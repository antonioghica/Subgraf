#include<iostream>
#include<fstream>
using namespace std;
ifstream f("subgraf1.in");
ofstream g("subgraf1.out");
int a[101][101],n,m,mn=1000000,b[101];
int main()
{
    int x,y,cnt=0,nr=0;
     f>>n;
     while(f>>x>>y)
     {
        if(a[x][y]==0){
         a[x][0]++;
         a[y][0]++;
         m++;
         }
       a[x][y]=1;
        a[y][x]=1;
     }
  for(int i=1;i<=n;i++)
    mn=min(a[i][0],mn);
     for(int i=1;i<=n;i++)
  {
      if(a[i][0]==mn)
        for(int j=1;j<=n;j++)
      {
          if(a[i][j]==1)
          {
              m--;
              a[i][j]=0;
          }
          a[j][i]=0;
      }
  }
      g<<m<<' ';

}
